<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:?after=additions">
         <command
               commandId="zeugen.ui.generateAll"
               label="Zeugnisse generieren"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file">
         <command
               commandId="zeugen.ui.generateAll"
               label="Zeugnisse generieren"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar">
         <command
               commandId="zeugen.ui.generateAll"
               label="Zeugnisse generieren"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="zeugen.ui.GenerateAllHandler"
            id="zeugen.ui.generateAll"
            name="Generiere Zeugnisse">
      </command>
   </extension>

</plugin>
